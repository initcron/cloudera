[desktop]
secret_key=<%= @options[:secret_key] %>
http_host=<%= @options[:http_host] %>
http_port=<%= @options[:http_port] %>
time_zone=America/Los_Angeles
django_debug_mode=0
http_500_debug_mode=0
server_user=<%= @options[:server_user] %>
server_group=<%= @options[:server_group] %>
[[auth]]
[[database]]
engine=<%= @options[:database_mysql] %>
host=<%= @options[:database_host] %>
port=<%= @options[:database_port] %>
user=<%= @options[:database_user] %>
password=<%= @options[:database_password] %>
name=<%= @options[:database_name] %>
[[smtp]]
host=localhost
port=25
user=
password=
tls=no
[[kerberos]]
[hadoop]
hadoop_home=/usr/lib/hadoop-0.20
[[hdfs_clusters]]
[[[default]]]
namenode_host=<%= @options[:hadoop_namenode_host] %>
hdfs_port=8020
http_port=50070
[[mapred_clusters]]
[[[default]]]
jobtracker_host=<%= @options[:hadoop_jobtracker_host] %>
